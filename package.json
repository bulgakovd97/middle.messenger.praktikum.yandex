{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "scripts": {
    "start": "npm run build && node server.js",
    "start:dev": "rimraf dist/ && npm run build:dev && node server.js",
    "start:prod": "npm run build:prod && node server.js",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "typecheck": "tsc -p tsconfig.json",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "check": "npm-run-all typecheck lint stylelint test",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bulgakovd97/middle.messenger.praktikum.yandex.git"
